<?xml version="1.0" encoding="UTF-8"?>
<model:Screen name="Login"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:model="http://wireframesketcher.com/1.0/model.ecore"
	theme="clean">

	<widgets name="Message" xsi:type="model:Text" id="7" x="200" y="80" width="267"
		height="64"
		text="Your account is blocked due to too many failed login attempts."
		measuredWidth="267" measuredHeight="64" foreground="#ff0000"
		dummyText="true">
		<font size="13px" italic="false" bold="false" underline="false" />
	</widgets>

	<widgets name="UsernameLabel" xsi:type="model:Label" id="2" x="120"
		y="176" text="Username:" measuredWidth="67" measuredHeight="23">
		<font size="13px" italic="false" bold="false" underline="false" />
	</widgets>
	<widgets name="Username" xsi:type="model:TextField" id="1" x="200"
		y="176" width="267" text="enter username" measuredWidth="267"
		measuredHeight="23">
		<font size="13px" italic="false" bold="false" underline="false" />
	</widgets>

	<widgets name="PasswordLabel" xsi:type="model:Label" id="3" x="123"
		y="232" text="Password:" measuredWidth="64" measuredHeight="23">
		<font size="13px" italic="false" bold="false" underline="false" />
	</widgets>

	<widgets name="Password" xsi:type="model:TextField" id="4" x="200"
		y="232" width="267" text="enter password" measuredWidth="267"
		measuredHeight="23">
		<font size="13px" italic="false" bold="false" underline="false" />
	</widgets>

	<widgets name="Submit" xsi:type="model:Button" id="5" x="414"
		y="288" text="Login" measuredWidth="53" measuredHeight="25">
		<font size="13px" italic="false" bold="false" underline="false" />
	</widgets>
	
	<widgets name="Logout" xsi:type="model:Button" id="6" x="0"
		y="0" text="Logout" measuredWidth="53" measuredHeight="25">
		<font size="13px" italic="false" bold="false" underline="false" />
	</widgets>
	

	<hRuler />
	<vRuler />
	<font size="13px" italic="false" bold="false" />
</model:Screen>
